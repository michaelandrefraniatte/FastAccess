{
    "manifest_version": 3,
    "name": "Fast Access Tab",
    "description": "An extension for open fast access page in a new tab.",
    "version": "0.0.0.1",
    "icons": {
        "16": "icons/wv16x16.png",
        "32": "icons/wv32x32.png",
        "64": "icons/wv64x64.png",
        "128": "icons/wv128x128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["favicon", "storage", "activeTab", "scripting"],
    "host_permissions": ["*://*/*", "file://*/*"],
    "homepage_url": "https://michaelandrefraniatte.github.io/FastAccess-Showcase"
}